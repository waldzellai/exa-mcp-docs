#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/b.c.nims/glassBead-MASTER/Exa/exa-docs-server/exa-mcp-docs/node_modules/.pnpm/tsup@8.5.0_postcss@8.5.6_tsx@4.20.3_typescript@5.8.3/node_modules/tsup/dist/node_modules:/Users/b.c.nims/glassBead-MASTER/Exa/exa-docs-server/exa-mcp-docs/node_modules/.pnpm/tsup@8.5.0_postcss@8.5.6_tsx@4.20.3_typescript@5.8.3/node_modules/tsup/node_modules:/Users/b.c.nims/glassBead-MASTER/Exa/exa-docs-server/exa-mcp-docs/node_modules/.pnpm/tsup@8.5.0_postcss@8.5.6_tsx@4.20.3_typescript@5.8.3/node_modules:/Users/b.c.nims/glassBead-MASTER/Exa/exa-docs-server/exa-mcp-docs/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/b.c.nims/glassBead-MASTER/Exa/exa-docs-server/exa-mcp-docs/node_modules/.pnpm/tsup@8.5.0_postcss@8.5.6_tsx@4.20.3_typescript@5.8.3/node_modules/tsup/dist/node_modules:/Users/b.c.nims/glassBead-MASTER/Exa/exa-docs-server/exa-mcp-docs/node_modules/.pnpm/tsup@8.5.0_postcss@8.5.6_tsx@4.20.3_typescript@5.8.3/node_modules/tsup/node_modules:/Users/b.c.nims/glassBead-MASTER/Exa/exa-docs-server/exa-mcp-docs/node_modules/.pnpm/tsup@8.5.0_postcss@8.5.6_tsx@4.20.3_typescript@5.8.3/node_modules:/Users/b.c.nims/glassBead-MASTER/Exa/exa-docs-server/exa-mcp-docs/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tsup/dist/cli-default.js" "$@"
else
  exec node  "$basedir/../tsup/dist/cli-default.js" "$@"
fi
