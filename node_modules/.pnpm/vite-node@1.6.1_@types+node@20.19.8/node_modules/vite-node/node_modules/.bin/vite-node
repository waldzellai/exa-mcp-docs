#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/b.c.nims/glassBead-MASTER/Exa/exa-docs-server/exa-mcp-docs/node_modules/.pnpm/vite-node@1.6.1_@types+node@20.19.8/node_modules/vite-node/node_modules:/Users/b.c.nims/glassBead-MASTER/Exa/exa-docs-server/exa-mcp-docs/node_modules/.pnpm/vite-node@1.6.1_@types+node@20.19.8/node_modules:/Users/b.c.nims/glassBead-MASTER/Exa/exa-docs-server/exa-mcp-docs/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/b.c.nims/glassBead-MASTER/Exa/exa-docs-server/exa-mcp-docs/node_modules/.pnpm/vite-node@1.6.1_@types+node@20.19.8/node_modules/vite-node/node_modules:/Users/b.c.nims/glassBead-MASTER/Exa/exa-docs-server/exa-mcp-docs/node_modules/.pnpm/vite-node@1.6.1_@types+node@20.19.8/node_modules:/Users/b.c.nims/glassBead-MASTER/Exa/exa-docs-server/exa-mcp-docs/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../vite-node.mjs" "$@"
else
  exec node  "$basedir/../../vite-node.mjs" "$@"
fi
